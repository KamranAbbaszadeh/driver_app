<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>BGTaskSchedulerPermittedIdentifiers</key>
		<array>
			<string>com.transistorsoft.fetch</string>
		</array>
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true/>
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>OMT</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>com.onemoretour</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<key>LSApplicationQueriesSchemes</key>
		<array>
		 	<string>whatsapp</string>
  			<string>tg</string>
			<string>comgooglemaps</string>
			<string>baidumap</string>
			<string>iosamap</string>
			<string>waze</string>
			<string>yandexmaps</string>
			<string>yandexnavi</string>
			<string>citymapper</string>
			<string>mapswithme</string>
			<string>osmandmaps</string>
			<string>dgis</string>
			<string>qqmap</string>
			<string>here-location</string>
			<string>tomtomgo</string>
			<string>copilot</string>
			<string>com.sygic.aura</string>
			<string>nmap</string>
			<string>kakaomap</string>
			<string>tmap</string>
			<string>szn-mapy</string>
			<string>mappls</string>
		</array>
		<key>LSRequiresIPhoneOS</key>
		<true/>
		<key>NSAppleMusicUsageDescription</key>
		<string>This app may access your media library to provide a customized audio experience if required by a feature or third-party plugin.</string>
		<key>NSBluetoothAlwaysUsageDescription</key>
		<string>This app uses Bluetooth to connect to nearby devices during the ride.</string>
		<key>NSCalendarsUsageDescription</key>
		<string>This app may access calendar data if required by scheduling features or third-party integrations.</string>
		<key>NSCameraUsageDescription</key>
		<string>We need access to your camera to take pictures.</string>
		<key>NSContactsUsageDescription</key>
		<string>This app may access your contacts to enhance communication features or integrations.</string>
		<key>NSLocalNetworkUsageDescription</key>
		<string>This app needs access to the local network for debugging and communication.</string>
		<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
		<string>This app requires location access even when not in use to provide background tracking for active rides.</string>
		<key>NSLocationAlwaysUsageDescription</key>
		<string>We need your location all the time to provide accurate services.</string>
		<key>NSLocationUsageDescription</key>
		<string>Location access is used to determine your current position and provide route navigation.</string>
		<key>NSLocationWhenInUseUsageDescription</key>
		<string>We use your location to show nearby pickup points and track your route during rides.</string>
		<key>NSMicrophoneUsageDescription</key>
		<string>We use the microphone for in-app voice features and communication during rides.</string>
		<key>NSMotionUsageDescription</key>
		<string>We use motion data to detect driving state and improve location accuracy.</string>
		<key>NSPhotoLibraryUsageDescription</key>
		<string>We need access to your photo library to upload images.</string>
		<key>NSSiriUsageDescription</key>
		<string>This app may interact with Siri for hands-free commands or shortcuts related to ride features.</string>
		<key>NSSpeechRecognitionUsageDescription</key>
		<string>This app may use speech recognition for voice input features or accessibility enhancements.</string>
		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true/>
		<key>FirebaseAppDelegateProxyEnabled</key>
		<true/>
		<key>UIBackgroundModes</key>
		<array>
			<string>fetch</string>
			<string>location</string>
			<string>processing</string>
			<string>remote-notification</string>
		</array>
		<key>NSAppTransportSecurity</key>
		<dict>
			<key>NSAllowsArbitraryLoads</key>
			<true/>
		</dict>
